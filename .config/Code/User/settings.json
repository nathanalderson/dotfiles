{
    "window.commandCenter": false,
    "window.titleBarStyle": "custom",
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Monokai Pro (Filter Machine)",
    "workbench.editor.showTabs": "single",
    "workbench.layoutControl.enabled": false,
    "breadcrumbs.symbolPath": "off",
    "editor.fontFamily": "'Fantasque Sans Mono', monospace",
    "editor.fontSize": 15,
    "editor.fontLigatures": "",
    "editor.formatOnSave": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "phase",
    "editor.cursorSurroundingLines": 3,
    "editor.minimap.enabled": false,
    "editor.find.loop": false,
    "editor.guides.indentation": false,
    "editor.inlineSuggest.enabled": true,
    "explorer.autoReveal": false,
    "explorer.decorations.badges": false,
    "extensions.ignoreRecommendations": true,
    "vscode-neovim.compositeKeys": {
        "jk": {
            "command": "vscode-neovim.escape",
        }
    },
    "files.autoSave": "onWindowChange",
    "elixirLS.envVariables": {
        "ELIXIR_WARNINGS_OK": "1"
    },
    "elixirLS.suggestSpecs": false,
    "exunit.saveAllBeforeRun": true,
    "[elixir]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.tabSize": 2,
    },
    "[html-eex]": {
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.html-language-features",
        "autoCreateQuotes": false,
        "html.autoClosingTags": false
    },
    "djlint.showInstallError": false,
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
        "editor.detectIndentation": false
    },
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "defaultInterpreterPath": "/bin/python"
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    // This allows ctrl-p to function as "previous" in the terminal
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    "terminal.integrated.stickyScroll.enabled": false,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.localEchoLatencyThreshold": 100,
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "debug.openDebug": "openOnDebugBreak",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off",
    },
    "git.confirmSync": false,
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "nathan-desktop-zt": "linux",
        "192.168.125.2": "macOS",
        "nathan-macbook-zt": "macOS",
        "nathan-macbook": "macOS",
        "nathan-desktop": "linux",
        "nathan-ubuntu2204": "linux",
        "10.0.100.107": "macOS",
        "10.0.100.182": "macOS",
        "10.0.100.60": "macOS",
        "MacBookAir": "macOS"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.diffAlgorithm": "advanced",
    "dart.showTestCodeLens": false,
    "dart.showMainCodeLens": false,
    "dart.showDartPadSampleCodeLens": false,
    "testing.automaticallyOpenPeekView": "never",
    "dart.closingLabels": false,
    "tailwindCSS.includeLanguages": {
        "elixir": "html",
        "phoenix-heex": "html",
    },
    "emmet.includeLanguages": {
        "phoenix-heex": "html",
        "elixir": "html",
    },
    "testing.gutterEnabled": false,
    "eslint.format.enable": true,
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.tree.indent": 12,
    "workbench.list.smoothScrolling": true,
    "editor.lineNumbers": "on",
    "workbench.activityBar.location": "top",
    "[liquid]": {
        "editor.defaultFormatter": "sissel.shopify-liquid",
        "editor.formatOnSave": true
    },
    "liquid.engine": "jekyll",
    "editor.smoothScrolling": true,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.mouseWheelScrollSensitivity": 0.4,
    "debug.javascript.autoAttachFilter": "disabled",
    "window.customTitleBarVisibility": "auto",
    "window.zoomLevel": -1.5682760965896694,
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
    "github.copilot.editor.enableAutoCompletions": true,
    "black-formatter.importStrategy": "fromEnvironment",
    "python.analysis.autoFormatStrings": true,
    "editor.suggest.shareSuggestSelections": true,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "workbench.preferredDarkColorTheme": "Monokai Pro (Filter Octagon)",
    "workbench.preferredLightColorTheme": "Monokai Pro Light",
    "workbench.editorAssociations": {
        "*.svg": "default"
    },
    "workbench.view.showQuietly": {
        "workbench.panel.output": true
    },
    "files.associations": {
        "*.html.j2": "html"
    },
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.genericParameterHints.const.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.renderColons": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.lens.implementations.enable": false,
    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.lens.debug.enable": false,
    "augment.chat.userGuidelines": "Don't apologize. Don't ask me about what I want to do next.",
    "github.copilot.nextEditSuggestions.enabled": true,
    "github.copilot.enable": {
        "plaintext": true,
        "markdown": true,
        "scminput": true
    },
    "github.copilot.selectedCompletionModel": "",
}
